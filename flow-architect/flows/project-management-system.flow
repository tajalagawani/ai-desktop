[node:FetchCardById]
type = "py"
label = "Fetch card with details"
code = """
import psycopg2

def fetch_card_with_details():
    db_url = "{{.Parameter.database_url}}"
    card_id = {{request_data.id_from_url}}

    conn = psycopg2.connect(db_url)
    cursor = conn.cursor()

    # Get card
    cursor.execute("SELECT id, list_id, title, description, position, due_date, priority, labels, created_at, updated_at FROM cards WHERE id = %s", (card_id,))
    card_row = cursor.fetchone()

    if not card_row:
        return {"error": "Card not found"}

    card = {
        "id": card_row[0],
        "list_id": card_row[1],
        "title": card_row[2],
        "description": card_row[3],
        "position": card_row[4],
        "due_date": str(card_row[5]) if card_row[5] else None,
        "priority": card_row[6],
        "labels": card_row[7],
        "created_at": str(card_row[8]),
        "updated_at": str(card_row[9]),
        "members": [],
        "comments": []
    }

    # Get assigned members
    cursor.execute("""
        SELECT m.id, m.name, m.email, m.avatar_url, cm.assigned_at
        FROM members m
        JOIN card_members cm ON m.id = cm.member_id
        WHERE cm.card_id = %s
    """, (card_id,))

    for member_row in cursor.fetchall():
        card["members"].append({
            "id": member_row[0],
            "name": member_row[1],
            "email": member_row[2],
            "avatar_url": member_row[3],
            "assigned_at": str(member_row[4])
        })

    # Get comments
    cursor.execute("""
        SELECT c.id, c.content, c.created_at, m.id, m.name, m.avatar_url
        FROM comments c
        JOIN members m ON c.member_id = m.id
        WHERE c.card_id = %s
        ORDER BY c.created_at DESC
    """, (card_id,))

    for comment_row in cursor.fetchall():
        card["comments"].append({
            "id": comment_row[0],
            "content": comment_row[1],
            "created_at": str(comment_row[2]),
            "author": {
                "id": comment_row[3],
                "name": comment_row[4],
                "avatar_url": comment_row[5]
            }
        })

    cursor.close()
    conn.close()

    return card
"""
function = "fetch_card_with_details"

