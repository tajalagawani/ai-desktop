[node:FetchBoardById]
type = "py"
label = "Fetch board with lists and cards"
code = """
import json
import psycopg2

def fetch_board_with_details():
    db_url = "{{.Parameter.database_url}}"
    board_id = {{request_data.id_from_url}}

    conn = psycopg2.connect(db_url)
    cursor = conn.cursor()

    # Get board
    cursor.execute("SELECT id, name, description, color, visibility, created_at, updated_at FROM boards WHERE id = %s", (board_id,))
    board_row = cursor.fetchone()

    if not board_row:
        return {"error": "Board not found"}

    board = {
        "id": board_row[0],
        "name": board_row[1],
        "description": board_row[2],
        "color": board_row[3],
        "visibility": board_row[4],
        "created_at": str(board_row[5]),
        "updated_at": str(board_row[6]),
        "lists": []
    }

    # Get lists
    cursor.execute("SELECT id, name, position FROM lists WHERE board_id = %s ORDER BY position", (board_id,))
    lists = cursor.fetchall()

    for list_row in lists:
        list_obj = {
            "id": list_row[0],
            "name": list_row[1],
            "position": list_row[2],
            "cards": []
        }

        # Get cards for this list
        cursor.execute("SELECT id, title, description, position, due_date, priority, labels FROM cards WHERE list_id = %s ORDER BY position", (list_row[0],))
        cards = cursor.fetchall()

        for card_row in cards:
            list_obj["cards"].append({
                "id": card_row[0],
                "title": card_row[1],
                "description": card_row[2],
                "position": card_row[3],
                "due_date": str(card_row[4]) if card_row[4] else None,
                "priority": card_row[5],
                "labels": card_row[6]
            })

        board["lists"].append(list_obj)

    cursor.close()
    conn.close()

    return board
"""
function = "fetch_board_with_details"

