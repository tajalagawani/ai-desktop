[workflow]
name = "Joke Generator"
description = "Generate a joke using OpenAI"
start_node = GenerateJoke

[node:GenerateJoke]
type = "openai"
label = "Generate joke with OpenAI"
model = "gpt-4"
temperature = 0.9
max_tokens = 150
messages = [
  {
    "role": "system",
    "content": "You are a comedian. Generate one short, clever joke."
  },
  {
    "role": "user",
    "content": "Tell me a joke"
  }
]

[env]
OPENAI_API_KEY
